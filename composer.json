{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "contentful/contentful": "^6.4",
        "fruitcake/laravel-cors": "^v2.2.0",
        "guzzlehttp/guzzle": "^7.4.2",
        "inertiajs/inertia-laravel": "^v0.5.4",
        "laravel/dusk": "^6.23",
        "laravel/framework": "^9.9",
        "laravel/horizon": "^v5.9.6",
        "laravel/jetstream": "^2.7",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.7",
        "spatie/laravel-csp": "^2.7",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "spatie/laravel-ignition": "^1.2",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^v6.1.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
